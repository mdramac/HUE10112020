   <TextField fx:id="departmentTextfield" layoutX="200.0" layoutY="36.0" />
   <Button fx:id="save" layoutX="200.0" layoutY="75.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Speichern" />
   <Button fx:id="cancel" layoutX="284.0" layoutY="75.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Abbrechen" />
   <ListView fx:id="listViewDepartments" layoutY="9.0" onMouseClicked="#listClicked" prefHeight="218.0" prefWidth="157.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
